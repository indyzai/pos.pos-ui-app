{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Indyz POS",
  "version": "0.1.5",
  "identifier": "com.indyzai.pos",
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Indyz POS",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "$APPLE_TEAM_ID"
    }
  }
}